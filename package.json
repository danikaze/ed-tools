{
  "name": "ed-tools",
  "version": "0.5.0",
  "description": "Parse the Elite Dangerous journal file to extract useful data",
  "main": "app/index.js",
  "scripts": {
    "start": "node app/ed-tools.js",
    "dev": "webpack --env=dev",
    "build": "webpack",
